{
    physics_body: {
        model: "blockbench/static/cube/25_cm",

        // Tungsten (pure, 99.95%) cube masses:
        // 1cm:   0.019
        // 5cm:   2.413
        // 10cm:  19.30
        // 25cm:  301.6
        // 50cm:  2412.5
        // 100cm: 19300
        mass: 301.6
    },

    particle_emitters: [
        {
            environment: {
                init: "v.radius = 0.6;"
            },
            lifetime: {
                lifetime_type: "looping",
                active_time: 1,
                sleep_time: 0
            },
            rate: {
                rate_type: "steady",
                spawn_rate: 30,
                max_count: 60
            },
            shape: {
                shape_type: "point"
            },
            offset: {
                x: "v.radius * -math.sin(v.emitter_age * 360)",
                y: "v.radius * math.cos(v.emitter_age * 360) + v.radius + 0.2",
                z: 0
            },
            particle: {
                environment: {
                    init: "v.scale_mult = 0.7;",

                    // Curves are evaluated before frame
                    curves: {
                        "v.size": {
                            type: "linear",
                            input: "v.age",
                            horizontal_range: "v.max_age",
                            nodes: [1, 0]
                        }
                    },
                    frame: "v.scaled_size = v.size * v.scale_mult"
                },

                model: "orbiter:blockbench/static/particle/dot",
                max_age: 1,
                billboard: "rotate_xyz",
                scale: ["v.scaled_size", "v.scaled_size", "v.scaled_size"]
            }
        },
        {
            lifetime: {
                lifetime_type: "looping",
                active_time: 1,
                sleep_time: 0
            },
            rate: {
                rate_type: "steady",
                spawn_rate: 1,
                max_count: 2
            },
            shape: {
                shape_type: "point"
            },
            offset: [0, 0.6, 0],
            particle: {
                environment: {
                    init: "v.scale_mult = 0.35;",

                    // Curves are evaluated before frame
                    curves: {
                        "v.size": {
                            type: "bezier",
                            input: "v.age",
                            horizontal_range: "v.max_age",
                            nodes: [0, 0, 5, 0]
                        }
                    },
                    frame: "v.scaled_size = v.size * v.scale_mult"
                },

                model: "orbiter:blockbench/static/particle/fireball/head_trail",
                max_age: 1,
                billboard: "rotate_xyz",
                scale: ["v.scaled_size", "v.scaled_size", "v.scaled_size"]
            }
        },
        {
            lifetime: {
                lifetime_type: "looping",
                active_time: 1,
                sleep_time: 0
            },
            rate: {
                rate_type: "steady",
                spawn_rate: 1,
                max_count: 2
            },
            shape: {
                shape_type: "point"
            },
            offset: [0, 1, 0],
            particle: {
                environment: {
                    init: "v.scale_mult = 0.35;",
                    curves: {
                        "v.size": {
                            type: "bezier_chain",
                            input: "v.age",
                            nodes: {
                                "0.09": {
                                    value: 0,
                                    slope: 8
                                },
                                "0.37": {
                                    value: 0,
                                    slope: -0.05
                                }
                            }
                        }
                    },
                    frame: "v.scaled_size = v.size * v.scale_mult"
                },

                model: "orbiter:blockbench/static/particle/fireball/head_trail",
                max_age: 1,
                billboard: "rotate_xyz",
                scale: ["v.scaled_size", "v.scaled_size", "v.scaled_size"]
            }
        }
    ]
}